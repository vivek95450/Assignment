#include <iostream>
#include <vector>
using namespace std;

int main() {
	
	vector<int> v = {1,2,2,4,5};
	int n;
	cout<<"Enter the number: ";
	cin>>n;
	v.push_back(n);
	for(int i = v.size()-2; i>=0 ;i--)
	{
		if(v[i]>n)
		{
			v[i+1] = v[i];
		}
		else
		{
			v[i+1] = n;
			break;
		}
	}
	for(int i = 0;i<v.size();i++)
		cout<<v[i]<<" ";
	return 0;
}
